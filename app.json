{
  "expo": {
    "name": "Quizzed",
    "slug": "season4",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/TheQuizLogoWhiteBackground.png",
    "splash": {
      "image": "./assets/splash_screen_Quizzed2_white.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.adaptive.quizapp",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "turboModules": true
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ]
  }
}
